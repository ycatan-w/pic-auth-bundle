{
  "name": "crayon/pic-auth-bundle",
  "description": "Symfony bundle integration for PicAuth - image-based authentication using a stamp system.",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "ycatan-w",
      "homepage": "https://github.com/ycatan-w"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:ycatan-w/pic-auth.git"
    }
  ],
  "require": {
    "php": "^8.2",
    "crayon/pic-auth": "^0.0",
    "symfony/framework-bundle": "^6.4 || ^7.0",
    "symfony/security-http": "^6.4 || ^7.0",
    "symfony/security-core": "^6.4 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "phpunit/php-code-coverage": "^12.3",
    "friendsofphp/php-cs-fixer": "^3.87",
    "symfony/maker-bundle": "^1.0",
    "symfony/yaml": "^6.4|^7.0",
    "symfony/security-bundle": "^6.4 || ^7.0",
    "symfony/twig-bundle": "^6.4 || ^7.0",
    "doctrine/doctrine-bundle": "^2.5.0"
  },
  "autoload": {
    "psr-4": {
      "Crayon\\PicAuthBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Crayon\\PicAuthBundle\\": "tests/"
    }
  }
}
